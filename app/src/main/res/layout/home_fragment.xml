    <!-- Invoice Card-->
    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="5dp"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:layout_below="@id/cardstats"
        android:layout_marginTop="4dp"
        android:id="@+id/recentinv"
        android:layout_marginBottom="4dp"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/recenttv"
                android:text="Recent Invoices"
                android:textColor="#ffffff"
                android:drawableLeft="@drawable/ic_ordermanage_white"
                android:drawablePadding="10dp"
                android:paddingTop="4dp"
                android:paddingBottom="4dp"
                android:textSize="22sp"
                android:paddingRight="10dp"
                android:paddingLeft="10dp"
                android:background="@drawable/home_card_back_gradient"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/recenthead"
                android:layout_alignParentRight="true"
                android:paddingTop="8dp"
                android:paddingBottom="6dp"
                android:paddingHorizontal="20dp"
                android:drawableRight="@drawable/ic_chevron_right_white_24dp"
                />
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/title"
                    app:contentPadding="5dp"
                    android:layout_below="@id/recenttv">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:orientation="horizontal"
                            >
                            <TextView
                                android:id="@+id/sno"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="#"
                                android:layout_weight="1.2"
                                android:textStyle="bold"
                                android:layout_marginLeft="3dp"
                                android:textColor="#000000"
                                android:textSize="17sp"

                                />

                            <TextView
                                android:id="@+id/invno"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="5dp"
                                android:layout_toRightOf="@id/sno"
                                android:maxLines="2"
                                android:text="Inv No."
                                android:textColor="#000000"
                                android:textSize="16sp" />
                            <TextView
                                android:id="@+id/amt"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="2"
                                android:text="Amount"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                            <TextView
                                android:id="@+id/otp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.1"
                                android:layout_marginRight="5dp"
                                android:maxLines="2"
                                android:text="OTP"
                                android:layout_marginLeft="5dp"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:textSize="16sp"
                                android:textAlignment="center"
                                android:textColor="#000000"
                                android:layout_weight="1"
                                android:text="History"
                                android:id="@+id/status"
                                />
                        </LinearLayout>

                    </LinearLayout>

                </android.support.v7.widget.CardView>
            <android.support.v7.widget.RecyclerView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/invrecyc"
                android:nestedScrollingEnabled="false"
                android:isScrollContainer="false"
                android:layout_marginTop="2dp"
                android:layout_below="@id/title"
                />
        </RelativeLayout>
    </android.support.v7.widget.CardView>
    <!--<RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/about_contact_lay"
        android:layout_below="@id/recentinv"
        >
        <TextView
